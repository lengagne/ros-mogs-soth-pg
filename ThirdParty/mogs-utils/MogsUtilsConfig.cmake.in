# - Config file for the MogsUtils lib
# It defines the following variables
#
# MogsUtils_INCLUDE_DIRS - include directories
# MogsUtils_LIBRARY_DIRS - library directory (normally not used!)
# MogsUtils_LIBRARIES - libraries to link against

# Tell the user project where to find our headers and libraries

SET(MogsUtils_INCLUDE_DIRS   @MogsUtils_INCLUDE_DIRS@)

SET(MogsUtils_LIBRARY_DIRS @MogsUtils_LIBRARY_DIRS@ @TCMALLOC_LIBRARY_DIRS@)

SET(MogsUtils_LIBRARIES @MogsUtils_LIBRARIES@ @TCMALLOC_LIBRARIES@)

SET(MogsUtils_FLAGS @MogsUtils_FLAGS@)

